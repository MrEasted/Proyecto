         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>BancoSOAP</servlet-name>
        <servlet-class>Interfaces.ws.soap.BancoSOAP</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BancoSOAP</servlet-name>
        <url-pattern>/BancoSOAP</url-pattern>
    </servlet-mapping>
</web-app>
